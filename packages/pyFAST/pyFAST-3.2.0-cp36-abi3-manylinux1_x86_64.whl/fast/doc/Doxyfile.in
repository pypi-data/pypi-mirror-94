PROJECT_NAME           = "@CMAKE_PROJECT_NAME@"
PROJECT_NUMBER         = "@VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_PATCH@"
PROJECT_BRIEF          = "Framework for Heterogeneous Medical Image Computing and Visualization"
STRIP_FROM_PATH        = @FAST_SOURCE_DIR@
INPUT                  = @PROJECT_SOURCE_DIR@/doc/main_page.md \
                         @FAST_SOURCE_DIR@
FILE_PATTERNS          = *.hpp
EXCLUDE_PATTERNS       = */Examples/* \
                         */Tests/* \
                         */Tools/* 
RECURSIVE              = YES
USE_MDFILE_AS_MAINPAGE = @PROJECT_SOURCE_DIR@/doc/main_page.md
CREATE_SUBDIRS         = YES
HTML_DYNAMIC_SECTIONS  = YES
