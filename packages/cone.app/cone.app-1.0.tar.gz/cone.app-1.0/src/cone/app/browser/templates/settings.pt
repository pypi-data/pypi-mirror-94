<tal:block xmlns:tal="http://xml.zope.org/namespaces/tal"
           xmlns:ajax="http://namesspaces.bluedynamics.eu/ajax"
           omit-tag="True">

  <ul class="nav nav-pills settingstabs"
      tal:define="tabs context.tabs">

    <li tal:repeat="tab tabs">
      <a href="#"
         ajax:target="${tab['target']}"
         tal:attributes="title tab['title']"
         tal:content="tab['title']">
        Title
      </a>
    </li>

  </ul>

  <div class="visualClear"></div>

  <div class="settingstabpane"></div>

</tal:block>