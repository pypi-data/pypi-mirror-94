                                            value='    ',
                                        ),
                                    ),
                                ),
                                whitespace_before_colon=SimpleWhitespace(
                                    value='',
                                ),
                                whitespace_after_colon=SimpleWhitespace(
                                    value=' ',
                                ),
                            ),
                            DictElement(
                                key=SimpleString(
                                    value="'title'",
                                    lpar=[],
                                    rpar=[],
                                ),
                                value=SimpleString(
                                    value=f"'{self.children}'",
                                    lpar=[],
                                    rpar=[],
                                ),
                                comma=MaybeSentinel.DEFAULT,
