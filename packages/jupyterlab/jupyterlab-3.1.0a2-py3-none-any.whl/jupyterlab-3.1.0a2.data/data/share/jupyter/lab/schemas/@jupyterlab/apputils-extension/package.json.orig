{
  "name": "@jupyterlab/apputils-extension",
  "version": "3.1.0-alpha.2",
  "description": "JupyterLab - Application Utilities Extension",
  "homepage": "https://github.com/jupyterlab/jupyterlab",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "sideEffects": [
    "style/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "style/*.css",
    "style/images/*.svg",
    "schema/*.json",
    "style/index.js"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "docs": "typedoc src",
    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.1.0-alpha.2",
    "@jupyterlab/apputils": "^3.1.0-alpha.2",
    "@jupyterlab/coreutils": "^5.1.0-alpha.2",
    "@jupyterlab/docregistry": "^3.1.0-alpha.2",
    "@jupyterlab/filebrowser": "^3.1.0-alpha.2",
    "@jupyterlab/mainmenu": "^3.1.0-alpha.2",
    "@jupyterlab/services": "^6.1.0-alpha.2",
    "@jupyterlab/settingregistry": "^3.1.0-alpha.2",
    "@jupyterlab/statedb": "^3.1.0-alpha.2",
    "@jupyterlab/translation": "^3.1.0-alpha.2",
    "@jupyterlab/ui-components": "^3.1.0-alpha.2",
    "@lumino/algorithm": "^1.3.3",
    "@lumino/commands": "^1.12.0",
    "@lumino/coreutils": "^1.5.3",
    "@lumino/disposable": "^1.4.3",
    "@lumino/polling": "^1.3.3",
    "@lumino/widgets": "^1.16.1",
    "es6-promise": "~4.2.8"
  },
  "devDependencies": {
    "rimraf": "~3.0.0",
    "typedoc": "~0.20.0-beta.27",
    "typescript": "~4.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  },
  "styleModule": "style/index.js",
  "gitHead": "72a522629b580390adc2bc7123d878e9493c0560"
}
