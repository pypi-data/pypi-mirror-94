    "@hookstate/persistence": "^3.0.0",
    "@meronex/icons": "^4.0.0",
    "color2k": "^1.1.1",
    "dagre": "^0.8.5",
    "dayjs": "^1.8.25",
    "framer-motion": "^3.2.2-rc.1",
    "lodash": "^4.17.15",
    "next": "^10.0.5",
    "react": "^17.0.1",
    "react-countdown": "^2.2.1",
    "react-device-detect": "^1.15.0",
    "react-dom": "^17.0.1",
    "react-fast-compare": "^3.2.0",
