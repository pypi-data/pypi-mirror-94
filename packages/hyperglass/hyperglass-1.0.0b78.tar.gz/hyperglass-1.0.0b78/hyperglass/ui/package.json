    "@types/react-select": "^3.0.28",
    "@types/react-table": "^7.0.25",
    "@types/string-format": "^2.0.0",
    "@types/yup": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
