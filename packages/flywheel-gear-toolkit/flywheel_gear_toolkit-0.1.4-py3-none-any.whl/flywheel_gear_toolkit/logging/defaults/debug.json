{
    "version": 1,
    "filters": {
        "out_filter": {
            "()": "PlaceHolder",
            "param": "noshow"
        },
        "err_filter": {
            "()": "PlaceHolder",
            "param": "noshow"
        }
    },
    "formatters": {
        "gear": {
            "format": "[ %(asctime)3s %(levelname)8s %(name)s: %(lineno)s - %(funcName)8s() ] %(message)s",
            "datefmt": "%Y%m%d %H:%M:%S"
        }
    },
    "handlers": {
        "gear": {
            "level": "DEBUG",
            "formatter": "gear",
            "filters": [
                "out_filter"
            ],
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stdout"
        },
        "gear_err": {
            "level": "DEBUG",
            "formatter": "gear",
            "filters": [
                "err_filter"
            ],
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stderr"
        }
    },
    "loggers": {
        "": {
            "handlers": [
                "gear",
                "gear_err"
            ],
            "level": "DEBUG"
        }
    },
    "disable_existing_loggers": false
}