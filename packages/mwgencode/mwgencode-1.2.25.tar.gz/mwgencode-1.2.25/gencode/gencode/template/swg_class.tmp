[
{% for cls in classes %}
				{
					"_type": "UMLClass",
					"_id": "cls_{{cls.id}}",
					"_parent": {
						"$ref": "{{swg_pkg_id}}"
					},
                    "documentation":"{{cls.doc}}",
					"name": "{{cls.name}}",
					"visibility": "public",
					"attributes": [
    {% for pro in cls.propertys %}
						{
							"_type": "UMLAttribute",
							"_id": "pro_{{pro.id}}",
							"_parent": {
								"$ref": "cls_{{cls.id}}"
							},
							"name": "{{pro.name}}",
                            "documentation":"{{pro.doc}}",
							"visibility": "public",
							"isStatic": false,
							"isLeaf": false,
							"type": "{{pro.type.lower()}}",
							"isReadOnly": false,
							"isOrdered": false,
							"isUnique": false,
							"isDerived": false,
							"aggregation": "none",
							"isID": false
						}{% if pro.id!=cls.propertys[-1].id %},{% endif %}
    {% endfor %}                                    
					],
					"isAbstract": false,
					"isFinalSpecialization": false,
					"isLeaf": false,
					"isActive": false
				}{% if cls.id != classes[-1].id %},{% endif %}
{% endfor %}
]